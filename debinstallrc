[debinstall]
group=devel
umask=0002

[create]
destination=/var/www/debian
configurations=/var/lib/debinstall

[upload]
check_signature=yes
run_lintian=yes
run_linda=yes

[publish]
signrepo=no
keyid=FFFFFF
check_signature=yes
run_lintian=yes
run_linda=yes

[archive]
archivedir=/var/www/debian/archive
