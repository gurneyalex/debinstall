[debinstall]
group=devel

[create]
destination=/var/www/debian
configurations=/var/lib/debinstall
default_distribution=sid

[upload]
check_signature=yes
checkers=lintian linda

[publish]
sign_repo=no
keyid=FFFFFF
check_signature=yes
checkers=lintian linda

[archive]
archivedir=/var/www/debian/archive
